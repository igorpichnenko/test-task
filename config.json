{
  "source": [
    "tokens/**/*.json"
  ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "transforms": [
        "attribute/cti",
        "name/cti/kebab",
        "size/px"
      ],
      "buildPath": "src/tokens/",
      "files": [
        {
          "destination": "_variables.scss",
          "format": "scss/variables"
        }
      ]
    }
  }
}